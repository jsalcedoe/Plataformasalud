	package com.js.plataformasalud.modelos.entidades;

import java.io.Serializable;

import jakarta.persistence.Entity;
import jakarta.persistence.Table;
import lombok.Getter;
import lombok.Setter;

@Getter
@Setter
@Entity
@Table(name="prestadorservicio")

/* Esta entidad es la encargada de crear la IPS prestadora de salud */

public class PrestadorServicio implements Serializable  {
	
	idprestadorservicio;
	nombreprestadorservicio
	nitprestadorservicio
	direccionprestadorservicio
	telefonoprestadorservicio
	representantelegaldireccionprestadorservicio
	tipodocrepresentantelegalprestadorservicio
	numdocprestadorservicio

	
	
	
	private static final long serialVersionUID = 1L;

}
